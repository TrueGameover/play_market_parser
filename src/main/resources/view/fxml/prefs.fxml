<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<?import javafx.geometry.Insets?>
<VBox maxHeight="700" maxWidth="700" minHeight="700" minWidth="700" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="playMarketParser.gui.PrefsController">
    <TitledPane animated="false" text="%general">
        <VBox spacing="10">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </TitledPane>
    <TitledPane animated="false" text="%posChecking">
        <VBox spacing="10">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </TitledPane>
    <TitledPane animated="false" text="%tipsCollecting">
        <VBox spacing="10">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </TitledPane>
    <HBox nodeOrientation="RIGHT_TO_LEFT" spacing="10">
        <Button mnemonicParsing="false" text="%ok" onAction="#onOkClick"/>
        <Button mnemonicParsing="false" text="%apply" onAction="#onApplyClick"/>
        <Button mnemonicParsing="false" text="%cancel" onAction="#onCancelClick"/>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
    </HBox>
</VBox>
