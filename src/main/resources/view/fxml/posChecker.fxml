<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.layout.VBox?>
<VBox fx:id="rootPane" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="playMarketParser.gui.PosCheckerController">
    <HBox spacing="10.0">
        <Button fx:id="addQueriesBtn" layoutX="450.0" layoutY="130.0" mnemonicParsing="false" onAction="#addQueriesClick" prefHeight="25.0" prefWidth="133.0" text="%addQueries" />
        <Button fx:id="importQueriesBtn" layoutX="450.0" layoutY="80.0" mnemonicParsing="false" onAction="#importQueriesClick" prefHeight="25.0" prefWidth="133.0" text="%importQueries" />
        <Button fx:id="startBtn" layoutX="450.0" layoutY="174.0" mnemonicParsing="false" onAction="#startPosChecking" prefHeight="25.0" prefWidth="133.0" text="%startPosChecking" />
        <Button fx:id="exportBtn" layoutX="450.0" layoutY="218.0" mnemonicParsing="false" onAction="#exportResults" prefHeight="25.0" prefWidth="133.0" text="%exportResults" />
        <CheckBox layoutX="480.0" layoutY="262.0" mnemonicParsing="false" text="CheckBox" />
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </HBox>
    <AnchorPane>
        <TableView fx:id="table" minHeight="200" AnchorPane.topAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
            <columns>
                <TableColumn fx:id="queryCol" text="%inputQuery" />
                <TableColumn fx:id="posCol" text="%position" />
            </columns>
        </TableView>
    </AnchorPane>
</VBox>
